{
  "name": "svelte.dev",
  "version": "1.0.0",
  "description": "Docs and examples for Svelte",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build --verbose",
    "preview": "svelte-kit preview",
    "copy-workers": "node scripts/copy-workers.js",
    "migrate": "node-pg-migrate -r dotenv/config",
    "update": "node scripts/update_template.js && node scripts/get-contributors.js",
    "start": "node build",
    "test": "mocha -r esm test/**",
    "deploy": "make deploy"
  },
  "dependencies": {
    "cookie": "^0.4.0",
    "devalue": "^2.0.0",
    "do-not-zip": "^1.0.0",
    "flru": "^1.0.2",
    "httpie": "^1.1.2",
    "jsonwebtoken": "^8.5.1",
    "marked": "^1.0.0",
    "pg": "^7.12.1",
    "prism-svelte": "^0.4.3",
    "prismjs": "^1.21.0",
    "yootils": "0.0.16"
  },
  "devDependencies": {
    "@sindresorhus/slugify": "^0.9.1",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/site-kit": "^1.4.0",
    "@sveltejs/svelte-repl": "^0.2.1",
    "dotenv": "^8.1.0",
    "esm": "^3.2.25",
    "jimp": "^0.8.0",
    "mocha": "^6.2.0",
    "node-fetch": "^2.6.1",
    "node-pg-migrate": "^3.22.0",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.3",
    "svelte": "^3.12.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
